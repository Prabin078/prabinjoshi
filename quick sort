#include<stdio.h>
#include<time.h>


void quicksort(long number[100000],long first,long last)
{
   	long i, j, pivot, temp;

   	if(first<last)
        {
     	 	 pivot=first;
      		 i=first;
      		 j=last;

     	 while(i<j)
         {
         	while(number[i]<=number[pivot]&&i<last)
           		 i++;
        	 while(number[j]>number[pivot])
            		j--;
         	if(i<j)
                {
            		temp=number[i];
            		number[i]=number[j];
            		number[j]=temp;
                }
         }

      	temp=number[pivot];
      	number[pivot]=number[j];
      	number[j]=temp;
      	quicksort(number,first,j-1);
      	quicksort(number,j+1,last);

        }
}

int main()
{
   	long i, n, A[100000];
	clock_t begin,end;
	double total_t;

   	printf("Enter the number of element to insert:\n");
   	scanf("%ld",&n);
	
   	//printf("Enter %ld elements: ", n);
   	for(i=0;i<n;i++)
      		//scanf("%ld",&A[i]);
		A[i]=rand(100000);

	begin=clock();
   	quicksort(A,0,n-1);
	end=clock();

   	printf("Order of Sorted elements:\n");
   	for(i=0;i<n;i++)
      		printf("%ld  \n",A[i]);
	printf("Execution time:\n");
	total_t=((double)(end-begin))/(CLOCKS_PER_SEC);
	printf("%lf\n",total_t);

   	return 0;
}
