#include<stdio.h>
#include<time.h>
void insertion_sort(long a[1000],long n)
{
long i,j;
long t;
for(i=1;i<=n-1;i++)
 {
  j=i;
  while(j>0 && a[j]<a[j-1])
  {
   t = a[j];
   a[j] = a[j-1];
   a[j-1] = t;
   j--;
  }
 }
};

int main()
{
 long n,a[1000],i,j;
 clock_t begin,end;
 double total_t;
 printf("Enter number of elements\n");
 scanf("%ld",&n);
 printf("Enter %ld longers\n",n);
 for (i=0;i<n;i++)
 //scanf("%ld",&a[i]);
 a[i]=rand(1000);
 begin=clock();
 insertion_sort(a,n);
 end=clock();
 printf("Sorted list in ascending order:\n");
 for(i=0;i<=n-1;i++)
 {
  printf("%ld\n",a[i]);
 }
 printf("execution time is:");
 total_t=((double)(end-begin)/(CLOCKS_PER_SEC));
 printf("%lf",total_t);
 return 0;
}
