#include<stdio.h>
#include<time.h>

void selectionSort(long a[], long size)
{
 long i, j;
 for(i=0;i<size;i++)
 {
  for(j=i;j<size;j++)
  {
   if(a[i] > a[j])
   swap(&a[i],&a[j]);
  }
 }
}

void swap(long *a, long *b)
{
 long temp;
 temp=*a;
 *a=*b;
 *b=temp;
}

int main()
{
 long a[100000],i,size;
 clock_t begin,end;
 double total_t;
 printf("How many numbers you want to sort:");
 scanf("\n%ld",&size);
 printf("\nEnter %ld number",size);
 for (i=0;i<size;i++)
 //scanf("%ld",&a[i]);
 a[i]=rand(100000);
 begin=clock();
 selectionSort(a,size);
 end=clock();
 printf("\nSorted array is");
 for (i=0;i<size;i++)
 printf("\n%ld",a[i]);
 printf("execution time taken is:");
 total_t=((double)(end-begin)/(CLOCKS_PER_SEC));
 printf("%lf\n",total_t);
 return 0;
}
